{
    "manifest_version": 3,
    "name": "Facebook Market Place",
    "version": "1.0",
    "description": "Sell Post Automation - Delowar Hossain",
    "permissions": [
      "activeTab", "scripting"
    ],
    "host_permissions": [
        "https://dummyjson.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "author": "Delowar Hossain [contact.delowar@gmail.com]",
    "homepage_url": "https://delowar.me"
  }
  